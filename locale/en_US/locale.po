msgid "plugins.generic.pfl.displayName"
msgstr "Publication Facts Label plugin"

msgid "plugins.generic.pfl.description"
msgstr "This plugin adds a sidebar block with statistics to indicate the journal's integrity characteristics to readers."

msgid "plugins.generic.pfl.metric"
msgstr "Metric"

msgid "plugins.generic.pflPlugin.settings.journal"
msgstr "Journal Information"

msgid "plugins.generic.pflPlugin.settings.excludePrior"
msgstr "Exclude by Date"

msgid "plugins.generic.pflPlugin.settings.dateStart"
msgstr "Start Date"

msgid "plugins.generic.pflPlugin.settings.dateStart.description"
msgstr "If your journal did not make use of OJS's editorial workflow prior to a certain date, specify it here in order to exclude prior submissions from the collection of statistics. The Publication Facts Label will not be presented on submissions prior to this date."

msgid "plugins.generic.pflPlugin.settings.dateStart.invalid"
msgstr "Please double-check that the specified start date is valid."

msgid "plugins.generic.pflPlugin.settings.academicSocietyUrl"
msgstr "URL"

msgid "plugins.generic.pflPlugin.settings.academicSocietyUrl.invalid"
msgstr "The academic society URL is not valid. Make sure the URL is complete and properly formatted."

msgid "plugins.generic.pflPlugin.fundingPluginMissing"
msgstr "Funding Plugin Not Present"

msgid "plugins.generic.pflPlugin.fundingPluginMissing.description"
msgstr "The Funding plugin is not present and enabled. In order for funding data to be presented by the Publication Facts Label, this plugin will need to be installed and enabled, and provided with the relevant data for each submission. Check the Plugin Gallery for this plugin."

msgid "plugins.generic.pflPlugin.settings.indexes"
msgstr "Journal's Index Listings"

msgid "plugins.generic.pflPlugin.settings.indexes.description"
msgstr "Under the heading \"Indexed\" in the Publication Facts Label (PFL), readers will be presented with up to four indexes in which the journal appears. Four of the indexes below offer broad coverage, while MEDLINE covers the health sciences and Latindex covers Latin America (see Info below). The indexes enable their journal listing to be publicly displayed (even when the index is otherwise closed). While two of the indexes can be automatically checked, the other four require you to search for the journal in the index and, if found, add it to the PFL."

msgid "plugins.generic.pflPlugin.settings.indexes.automatic"
msgstr "Automated Listing of Indexes in the PFL"

msgid "plugins.generic.pflPlugin.settings.indexes.automatic.doaj"
msgstr "Directory of Open Access Journals"

msgid "plugins.generic.pflPlugin.settings.indexes.automatic.doaj.validationFailed"
msgstr "The journal's indexing in DOAJ could not be verified. Ensure that your journal is indexed and the appropriate ISSN is configured in Journal Settings."

msgid "plugins.generic.pflPlugin.settings.indexes.automatic.scholar"
msgstr "Google Scholar"

msgid "plugins.generic.pflPlugin.settings.indexes.automatic.medline"
msgstr "MEDLINE"

msgid "plugins.generic.pflPlugin.settings.indexes.automatic.medline.validationFailed"
msgstr "The journal's indexing in MEDLINE could not be verified. Ensure that your journal is indexed and the appropriate ISSN is configured in Journal Settings."

msgid "plugins.generic.pflPlugin.settings.indexes.automatic.latindex"
msgstr "Latindex"

msgid "plugins.generic.pflPlugin.settings.indexes.automatic.latindex.validationFailed"
msgstr "The journal's indexing in Latindex could not be verified. Ensure that your journal is indexed and the appropriate ISSN is configured in Journal Settings."

msgid "plugins.generic.pflPlugin.settings.indexes.manual"
msgstr "Manual Listing of Indexes in the PFL"

msgid "plugins.generic.pflPlugin.settings.indexes.manual.scopus"
msgstr "<strong>Scopus</strong> (<a target=\"_new\" href=\"https://en.wikipedia.org/wiki/Scopus\">Info</a>)"

msgid "plugins.generic.pflPlugin.settings.indexes.manual.scopus.urlInvalid"
msgstr "The Scopus URL you entered is not correct. Please read the instructions and try again."

msgid "plugins.generic.pflPlugin.settings.indexes.manual.scopus.step1"
msgstr "Go to Scopus' <a href=\"https://www.scopus.com/sources.uri?zone=TopNavBar&origin=\" target=\"_new\">Sources</a> page and select \"Title\" from the pulldown menu."

msgid "plugins.generic.pflPlugin.settings.indexes.manual.scopus.step2"
msgstr "Enter the journal title and if indexed, it will appear as a \"Result.\""

msgid "plugins.generic.pflPlugin.settings.indexes.manual.scopus.step3"
msgstr "Click on the title and paste the resulting page's URL here:"

msgid "plugins.generic.pflPlugin.settings.indexes.manual.scopus.step4"
msgstr "If the journal is not indexed, here is the <a target=\"_new\" href=\"https://www.elsevier.com/solutions/scopus/how-scopus-works/content/content-policy-and-selection\">process</a> for inclusion."

msgid "plugins.generic.pflPlugin.settings.indexes.manual.wos"
msgstr "<strong>Web of Science</strong> (<a target=\"_new\" href=\"https://en.wikipedia.org/wiki/Web_of_Science\">Info</a>)"

msgid "plugins.generic.pflPlugin.settings.indexes.manual.wos.urlInvalid"
msgstr "The Web of Science URL you entered is not correct. Please read the instructions and try again."

msgid "plugins.generic.pflPlugin.settings.indexes.manual.wos.step1"
msgstr "Go to the <a href=\"https://mjl.clarivate.com/search-results\" target=\"_new\">Web of Science</a> and enter the name of the journal in the search box."

msgid "plugins.generic.pflPlugin.settings.indexes.manual.wos.step2"
msgstr "If indexed in the Web of Science, it will appear as a \"Result.\""

msgid "plugins.generic.pflPlugin.settings.indexes.manual.wos.step3"
msgstr "Copy the URL by \"Share This Journal\" and paste it here:"

msgid "plugins.generic.pflPlugin.settings.indexes.manual.wos.step4"
msgstr "If the journal is not indexed, here is the <a target=\"_new\" href=\"https://clarivate.com/products/scientific-and-academic-research/research-discovery-and-workflow-solutions/webofscience-platform/web-of-science-core-collection/editorial-selection-process/\">process</a> for inclusion."

